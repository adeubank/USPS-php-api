{
    "name": "adeubank/usps-php-api",
    "type": "library",
    "description": "Symfony-compatible PHP wrapper for USPS Web Tools API",
    "keywords": ["usps", "api"],
    "authors": [
        {
            "name": "Vincent Gabriel",
            "homepage": "http://vadimg.com",
            "role": "Developer"
        },
        {
            "name": "Peter Adams",
            "homepage": "https://www.pingdevelopment.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": "2.*"
    },
    "autoload": {
        "psr-0": {
            "USPS\\": "src/",
            "Utility\\": "src/"
        },
        "psr-4": {
            "USPS\\": "/",
            "Utility\\": "/"
        }
    }
}
